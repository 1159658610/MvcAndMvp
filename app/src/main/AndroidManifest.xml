                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvc.demo1.MvcActivity" />
        <activity android:name=".mvc.demo3.StartupActivity"></activity>
